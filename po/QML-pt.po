msgid "Type of the random effects of the mean vectors."
msgstr "Tipo de efeitos aleatórios dos vetores médios."

msgctxt "MetaAnalyticSem|"
msgid "Sigma - Mu"
msgstr "Sigma - Mu"

msgctxt "MetaAnalyticSem|"
msgid "Full"
msgstr "Completo"

msgctxt "MetaAnalyticSem|"
msgid ""
"Type of the random effects between the correlation/covariance vectors and "
"the mean vectors."
msgstr ""
"Tipo de efeitos aleatórios entre os vetores de correlação/covariância e os "
"vetores médios."

msgctxt "MetaAnalyticSem|"
msgid "Pairwise model comparison"
msgstr "Comparação de modelos em pares"

msgctxt "MetaAnalyticSem|"
msgid "Show a pairwise model comparison table."
msgstr "Mostrar uma tabela de comparação de modelos em pares."

msgctxt "MetaAnalyticSem|"
