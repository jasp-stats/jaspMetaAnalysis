msgid "Identity"
msgstr "முற்றொருமை"

msgctxt "ClassicalMetaAnalysisMultilevelMultivariate|"
msgid "Diagonal"
msgstr "மூலைவிட்டம்"

msgctxt "ClassicalMetaAnalysisMultilevelMultivariate|"
msgid "AR(1)"
msgstr "Ar (1)"

msgctxt "ClassicalMetaAnalysisMultilevelMultivariate|"
